<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Contactos</title>
    <link rel="stylesheet" href="stylesheets/style.css">
</head>
<body>
    
    <div class="Content-father table">
        <table>
            <tr>
                <th>Nombre</th>
                <th>Correo</th>
                <th>Comentario</th>
                <th>IP</th>
                <th>Fecha/Hora </th>
                <th>Pais</th>
            </tr>
            <% for (const datos of get) { %>
            <tr>
                <th><%= datos.name%></th>
                <td><%= datos.email%></td>
                <td><%= datos.commentary%></td>
                <td><%= datos.ipaddress%></td>
                <td><%= datos.date%></td>
                <td><%= datos.country%></td>
            </tr>
            <% } %>
        </table>
    </div>
</body>
</html>
